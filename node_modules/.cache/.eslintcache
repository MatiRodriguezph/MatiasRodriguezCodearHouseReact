tyle.display = \"block\"\n            setTimeout(() => {\n                resolve(arrayProductos)\n                document.getElementById(\"spinner\").style.display = \"none\"\n            }, 2000)\n        })\n\n        promesa.then(res => {\n            if (parametros.id == 1){\n                res = res.filter(a => a.categoria == \"Notebooks\");\n            }\n\n            if (parametros.id == 2){\n                res = res.filter(a => a.categoria == \"Perifericos\");\n            }\n\n            if (parametros.id == 3){\n                res = res.filter(a => a.categoria == \"Sillas gamer\");\n            }\n\n            setProductos(res)\n        })\n\n    }, [parametros])\n    \n    return (    \n        <>\n            <Spinner id=\"spinner\" animation=\"grow\" />\n            <ItemList items={productos}/>\n        </>\n    )\n}\n\nexport default ItemListContainer","C:\\Users\\Mati\\Desktop\\appreact1 Matias Rodriguez\\src\\components\\Cart\\Cart.js",[],"C:\\Users\\Mati\\Desktop\\appreact1 Matias Rodriguez\\src\\components\\ItemDetail\\ItemDetailContainer.js",["135","136"],"import { useState, useEffect } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport ItemDetail from \"./ItemDetail\";\nimport { arrayProductos } from \"../../datos\";\n\nconst ItemDetailContainer = () => {\n\n    const [producto, setProducto] = useState({})\n    const parametros = useParams()\n\n    //console.log(props)\n\n    useEffect(() => {\n\n        const promesa = new Promise((resolve, reject) => {\n            resolve(arrayProductos)\n        })\n        \n        promesa.then(res => {\n            setProducto(res.find(r => r.id == parametros.id))\n        })\n\n    }, [])\n\n    return (\n        <ItemDetail producto={producto}/>\n    )\n}\n\nexport default ItemDetailContainer","C:\\Users\\Mati\\Desktop\\appreact1 Matias Rodriguez\\src\\components\\Header\\NavBar.js",[],"C:\\Users\\Mati\\Desktop\\appreact1 Matias Rodriguez\\src\\components\\ItemList\\ItemLi